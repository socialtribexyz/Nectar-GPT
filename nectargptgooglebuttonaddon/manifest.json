{
    "name": "I'm Feeling NectarGPT",
    "action": {},
    "manifest_version": 3,
    "version": "1.0",
    "description": "Sometimes you get a feeling that you don't want to google it.",
    "permissions": [
      "tabs",
      "scripting",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["images/chatgpt-logo.png"],
        "matches": ["<all_urls>"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
}